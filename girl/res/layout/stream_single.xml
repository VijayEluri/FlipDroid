<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:wp="http://schemas.android.com/apk/res/com.goal98.girl"
              xmlns:android="http://schemas.android.com/apk/res/android" android:layout_height="fill_parent"
              android:layout_width="fill_parent" android:orientation="vertical">
    <com.goal98.girl.view.TopBar android:id="@+id/topbar" wp:rightButtonImage="@drawable/like"
                                      android:layout_height="wrap_content"
                                      android:layout_width="fill_parent"
                                      wp:title="@string/allfeeds"></com.goal98.girl.view.TopBar>
    <com.srz.androidtools.viewpagertitle.ViewPagerTabs
            android:id="@+id/tabs"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#FFF0F0F0" />
    <View
            android:layout_width="fill_parent"
            android:layout_height="2dip"
            android:background="#FF9596a3" />
    <android.support.v4.view.ViewPager
            android:id="@+id/viewpager"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">


    </android.support.v4.view.ViewPager>



</LinearLayout>
